# Patreon OAuth Credentials for Server-Side API Routes
PATREON_CLIENT_ID=your_client_id
PATREON_CLIENT_SECRET=your_client_secret
PATREON_REDIRECT_URI=http://localhost:3000/api/auth/callback/patreon

# Patreon OAuth Credentials for Client-Side Components (with NEXT_PUBLIC_ prefix)
# Note: In a production app, you would use these in client components
# For this demo, we've hardcoded values in the page component
NEXT_PUBLIC_PATREON_CLIENT_ID=your_client_id
NEXT_PUBLIC_PATREON_REDIRECT_URI=http://localhost:3000/api/auth/callback/patreon

# NextAuth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret
